{
    "name": "eyeable/eye-able-oxid",
    "description": "",
    "type": "oxideshop-module",
    "keywords": [
        "oxid",
        "modules",
        "eShop"
    ],
    "license": [
        "GPL-3.0"
    ],
    "prefer-stable": true,
    "prefer-dist": true,
    "require": {
        "php": "^8.0 | ^8.1",
        "symfony/filesystem": "^6.0"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpstan/phpstan": "^1.9.14",
        "squizlabs/php_codesniffer": "3.*",
        "phpmd/phpmd": "^2.11",
        "oxid-esales/oxideshop-ce": "dev-b-7.0.x",
        "phpunit/phpunit": "^9.6",
        "mikey179/vfsstream": "~1.6.8"
    },
    "conflict": {
        "oxid-esales/oxideshop-ce": "<7.0"
    },
    "autoload": {
        "psr-4": {
            "EyeAble\\EyeAbleAssist\\": "src/",
            "EyeAble\\EyeAbleAssist\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests-integration": "XDEBUG_MODE=coverage vendor/bin/phpunit --bootstrap=/var/www/source/bootstrap.php --config=tests/ --testsuite=Integration",
        "tests-coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit --coverage-text --bootstrap=/var/www/source/bootstrap.php --config=tests/ --coverage-html=tests/result/coverage",

        "tests-codeception": "(cd /var/www/ && THEME_ID=apex MODULE_IDS=eyeable_assist /var/www/vendor/bin/codecept run acceptance -c /var/www/vendor/eyeable/eye-able-oxid/tests/codeception.yml )",

        "tests-all": [
            "@tests-integration",
            "@tests-codeception"
        ]
    },
    "config": {
        "allow-plugins": {
            "oxid-esales/oxideshop-composer-plugin": true,
            "oxid-esales/oxideshop-unified-namespace-generator": true
        }
    }
}
